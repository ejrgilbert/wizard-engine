component PathUtil {
	def extractName(path: string) -> string {
        var sb = StringBuilder.new();
        for (c in path) {
            if (c == '/') {
                sb.reset();
            } else {
                sb.putc(c);
            }
        }
        var name = sb.toString();
        name = Arrays.range(name, 0, name.length - 5);
        return name;
    }
}